# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true

# urf-8 character set
charset = utf-8

# 2 space indentation
indent_style = space
indent_size = 2

# Remove any whitespace characters preceding newline characters
trim_trailing_whitespace = true
